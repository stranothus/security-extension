{
    "manifest_version" : 2,
    "name" : "Security Extension",
    "version" : "0.1.0",
    "description" : "Ensure better internet security",
    "permissions": [
        "activeTab",
        "tabs",
        "declarativeContent",
        "storage"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["url-check.js"],
            "css": ["url-check.css"]
        }
    ]
}
